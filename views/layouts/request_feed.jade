extend ../layout2
block content
  .row
    .col-sm-1.greybackground
      a.btn.homebtn(href='/request_feed')
        img#logomini(src='/img/maroon_shadow.png')
    .col-sm-7
      //- .form-inline
      //- #titleheading2 Give It Forward    
        //- br
      #titleheading2 Welcome, CS_Nerd  
        //- img#profilepicmini(src="/img/smile.png")   
    .col-sm-4.padding-top  
      a.leftsmallroundbtn.btn.winebtn(href='/request_feed') RF
      a.middlesmallroundbtn.btn.greybtn.btn-secondary(href='/orgs') Orgs
      a.middlesmallroundbtn.btn.greybtn.btn-secondary(href='/request_feed') Notes
      a.rightsmallroundbtn.btn.greybtn(href='/my_profile') My Profile
  .row.bottom-row.padding-top
    .col-sm-9
      .form-inline     
        input.round(type='text', placeholder='search for tags')
        a.btn.roundminibtn.winebtn(href='/request_feed') go
